{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/public/$1" },
    { "src": "https://open-os.vercel.app/auth/github", "dest": "/server.js" },
    { "src": "/auth/github/callback", "dest": "/server.js" }
  ]
}
